{
  "manifest_version": 2,
  "name": "Fansubs for Youtube",
  "version": "3",
  "description": "Fansubs styles for Youtube captions",
  "web_accessible_resources": ["*.ttf" ],
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },

  "content_scripts": [
    {
      "css": ["fansub.css"],
      "matches": ["<all_urls>"]
    }
  ]
}
